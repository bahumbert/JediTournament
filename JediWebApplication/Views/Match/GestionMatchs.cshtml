
@{
    ViewBag.Title = "GestionMatchs";
}

<h2>GestionMatchs</h2>
<div class="stars"></div>
<div class="twinkling"></div>
<div class="clouds"></div>
<div class="row">
    <div class="col-md-12">
        <table class="table tableau">
            <thead>
                <tr>
                    <th>Jedi Vainqueur</th>
                    <th>Jedi1</th>
                    <th>Jedi2</th>
                    <th>Stade</th>
                </tr>
            </thead>
            @foreach (var match in ViewBag.Matchs)
            {
                <tbody>
                    <tr>
                        <th>@match.IdJediVainqueur</th>
                        <td>@match.Jedi1.Nom</td>
                        <td>@match.Jedi2.Nom</td>
                        <td>@match.Stade.Nom</td>
                        <td><a href="./Editer/@match.Id">Modifier</a></td>
                        <td><a href="./Supprimer/@match.Id">Supprimer</a></td>
                    </tr>
                </tbody>
            }
            <tr>
                <td><a href="./Ajouter/">Ajouter</a></td>
            </tr>
        </table>
    </div>
</div>
